{
  "name": "my-board-for-github",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "korefile": "^1.1.0",
    "immer": "^7.0.7",
    "klona": "^2.0.1",
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.1",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "graphql": "^15.3.0",
    "graphql-request": "^3.0.0",
    "lodash-es": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-scripts": "3.4.3",
    "react-trello": "^2.2.8",
    "typescript": "~3.9.7"
  },
  "devDependencies": {
    "@types/react-redux": "^7.1.9",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  }
}
